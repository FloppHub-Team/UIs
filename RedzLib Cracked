local C=B[1]
local D=B[2]

B=function(E)
C[D]=E
end
end

return table.pack(B)end local C=function(


C)
A[C]=nil end local D=function(


D, ...)
if A[D]then return end

A[D]=true

for E=1,#D do
task.spawn(D[E],...)
end

task.defer(C,D)end


