function ad:GetNoSelfCall(ar)
assert(type(ar)=="string",`"Tab.GetNoSelfCall". 'string' expected, got {typeof(ar)}`)
local as=self[ar]
assert(type(as)=="function",`"Tab.GetNoSelfCall". '{as}' is not a 'function'-{ar}`)

return function(...)
return as(self,...)
end
end

