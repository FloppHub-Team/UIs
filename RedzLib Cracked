return setmetatable({
Title=ar.Text,
Description=as.Text,
DESCRIPTION_LABEL=as,
TITLE_LABEL=ar,

CALLBACKS=av,
DESTROY_ELEMENT=at,
VISIBLE_ELEMENT=at,

TEXTBOX=au,
BUTTON=at,

Parent=aq,
Kind="TextBox"
},ai)
end

