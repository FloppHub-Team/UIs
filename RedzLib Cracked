aQ(aY)end local aZ=function(


aZ, a_)
if a_ and type(aZ)=="boolean"then
return aZ==true and aN[a_]
end

return if type(aZ)=="number"then aN[aZ]else aO[tostring(aZ)]end local a_=function(


