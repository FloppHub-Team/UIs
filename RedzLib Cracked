ar.Elements.Corner=ar.Corner
ar.Corner=nil
end

if ar.Stroke then
ar.Elements.Stroke=ar.Stroke
ar.Stroke=nil
end

y.Draggable(as,_,0.5)
y.SetProperties(as,ar)

return as
end

